swe_ao: swe_ao {
        compatible = "zmk,behavior-macro";
        label = "swe ao";
        #binding-cells = <0>;
        bindings
          = <&macro_press   &kp LALT>
          , <&macro_tap     &kp KP_N0 &kp KP_N2 &kp KP_N2 &kp KP_N9>
          , <&macro_release &kp LALT>;
    };
    swe_ao_shift: swe_ao_shift {
        compatible = "zmk,behavior-macro";
        label = "swe ao shift";
        #binding-cells = <0>;
        bindings
          = <&macro_press   &kp LALT>
          , <&macro_tap     &kp KP_N0 &kp KP_N1 &kp KP_N9 &kp KP_N7>
          , <&macro_release &kp LALT>;
    };

